{
  "name": "gocommander",
  "version": "1.0.2",
  "description": "A Go implementation for command-line interfaces with Node.js compatibility",
  "main": "index.js",
  "scripts": {
    "build": "node-gyp configure build",
    "clean": "node-gyp clean",
    "rebuild": "node-gyp rebuild",
    "go:build": "node scripts/build-go.js",
    "go:build-clean": "node scripts/build-go.js --clean",
    "go:build-verbose": "node scripts/build-go.js --verbose",
    "install": "node scripts/install.js",
    "postinstall": "node scripts/postinstall.js",
    "test": "node test/test.js",
    "test:integration": "node test/cpp-addon-bridge-integration-test.js",
    "test:js-integration": "node test/javascript-integration-test.js",
    "test:go-integration": "node test/go-backend-integration-test.js",
    "test:build-system": "node test/build-system-test.js",
    "test:all": "npm run test && npm run test:integration && npm run test:js-integration && npm run test:go-integration && npm run test:build-system",
    "advanced-test": "node test/advanced-test.js",
    "validate:deps": "node -e \"require('./scripts/install.js').validateDependencies()\"",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "publish-version": "git push && git push --tags && npm publish"
  },
  "keywords": [
    "gocommander",
    "commander",
    "cli",
    "command",
    "option",
    "parser",
    "go"
  ],
  "author": "Rohit Soni",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohitsoni-dev/gocommander.git"
  },
  "bugs": {
    "url": "https://github.com/rohitsoni-dev/gocommander/issues"
  },
  "homepage": "https://github.com/rohitsoni-dev/gocommander#readme",
  "dependencies": {
    "node-addon-api": "^7.0.0"
  },
  "devDependencies": {
    "node-gyp": "^10.0.0"
  },
  "gypfile": true,
  "files": [
    "index.js",
    "lib/",
    "src/",
    "binding.gyp",
    "scripts/",
    "examples/",
    "test/",
    "README.md",
    "LICENSE",
    "package.json"
  ]
}
